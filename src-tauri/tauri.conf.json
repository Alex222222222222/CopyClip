{
  "build": {
    "beforeDevCommand": "bash ./scripts/tailwindcss.sh && trunk serve",
    "beforeBuildCommand": "bash ./scripts/tailwindcss.sh && trunk build --release && bash ./scripts/wasm-opt.sh",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "../public/icons/32x32.png",
      "../public/icons/128x128.png",
      "../public/icons/128x128@2x.png",
      "../public/icons/icon.icns",
      "../public/icons/icon.ico"
    ],
    "resources": [
      "../ocr_models/*"
    ],
    "targets": "all"
  },
  "productName": "CopyClip",
  "mainBinaryName": "CopyClip",
  "version": "0.3.10",
  "identifier": "org.eu.huazifan.copyclip",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "search",
        "title": "Copy Clip",
        "url": "search",
        "width": 1280,
        "height": 800,
        "useHttpsScheme": true
      }
    ],
    "trayIcon": {
      "iconPath": "../public/icons/icon.png",
      "iconAsTemplate": true
    }
  }
}